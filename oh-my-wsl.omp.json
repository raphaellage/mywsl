{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "alignment": "left",
        "segments": [
          {
            "foreground": "lightGreen",
            "leading_diamond": "\u256d\u2500",
            "style": "diamond",
            "template": " {{.Icon}} ",
            "type": "os"
          },
          {
            "foreground": "#d3d7cf",
            "style": "plain",
            "template": "{{ .UserName }} ",
            "type": "session"
          },
          {
            "foreground": "cyan",
            "properties": {
              "style": "folder"
            },
            "style": "plain",
            "template": "<#ffffff>in</> {{ .Path }} ",
            "type": "path"
          },
          {
            "foreground": "#ff94df",
            "properties": {
              "fetch_status": true
            },
            "style": "plain",
            "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }} ",
            "type": "git"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "right",
        "segments": [
          {
            "foreground": "#ffffff",
            "properties": {
              "command": "date +%H:%M:%S",
              "shell": "bash"
            },
            "style": "plain",
            "template": " {{ .Output }} ",
            "type": "command"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "foreground": "#d3d7cf",
            "style": "plain",
            "template": "\u2570\u2500",
            "type": "text"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "segments": [
          {
            "foreground": "lightGreen",
            "style": "plain",
            "template": "\u276f",
            "type": "text"
          }
        ],
        "type": "prompt"
      }
    ],
    "final_space": true,
    "version": 2
  }